    <script  >
       var slideshowDuration = 4000;
    var slideshow=$('.main-content1 .slideshow1');
    
    function slideshowSwitch(slideshow,index,auto){
      if(slideshow.data('wait')) return;
    
      var slides = slideshow.find('.slide1');
      var pages = slideshow.find('.pagination1');
      var activeSlide = slides.filter('.is-active1');
      var activeSlideImage = activeSlide.find('.image-container1');
      var newSlide = slides.eq(index);
      var newSlideImage = newSlide.find('.image-container1');
      var newSlideContent = newSlide.find('.slide-content1');
      var newSlideElements=newSlide.find('.caption1 > *');
      if(newSlide.is(activeSlide))return;
    
      newSlide.addClass('is-new1');
      var timeout=slideshow.data('timeout');
      clearTimeout(timeout);
      slideshow.data('wait',true);
      var transition=slideshow.attr('data-transition');
      if(transition=='fade'){
        newSlide.css({
          display:'block',
          zIndex:2
        });
        newSlideImage.css({
          opacity:0
        });
    
        TweenMax.to(newSlideImage,1,{
          alpha:1,
          onComplete:function(){
            newSlide.addClass('is-active1').removeClass('is-new1');
            activeSlide.removeClass('is-active1');
            newSlide.css({display:'',zIndex:''});
            newSlideImage.css({opacity:''});
            slideshow.find('.pagination1').trigger('check');
            slideshow.data('wait',false);
            if(auto){
              timeout=setTimeout(function(){
                slideshowNext(slideshow,false,true);
              },slideshowDuration);
              slideshow.data('timeout',timeout);}}});
      } else {
        if(newSlide.index()>activeSlide.index()){
          var newSlideRight=0;
          var newSlideLeft='auto';
          var newSlideImageRight=-slideshow.width()/8;
          var newSlideImageLeft='auto';
          var newSlideImageToRight=0;
          var newSlideImageToLeft='auto';
          var newSlideContentLeft='auto';
          var newSlideContentRight=0;
          var activeSlideImageLeft=-slideshow.width()/4;
        } else {
          var newSlideRight='';
          var newSlideLeft=0;
          var newSlideImageRight='auto';
          var newSlideImageLeft=-slideshow.width()/8;
          var newSlideImageToRight='';
          var newSlideImageToLeft=0;
          var newSlideContentLeft=0;
          var newSlideContentRight='auto';
          var activeSlideImageLeft=slideshow.width()/4;
        }
    
        newSlide.css({
          display:'block',
          width:0,
          right:newSlideRight,
          left:newSlideLeft
          ,zIndex:2
        });
    
        newSlideImage.css({
          width:slideshow.width(),
          right:newSlideImageRight,
          left:newSlideImageLeft
        });
    
        newSlideContent.css({
          width:slideshow.width(),
          left:newSlideContentLeft,
          right:newSlideContentRight
        });
    
        activeSlideImage.css({
          left:0
        });
    
        TweenMax.set(newSlideElements,{y:20,force3D:true});
        TweenMax.to(activeSlideImage,1,{
          left:activeSlideImageLeft,
          ease:Power3.easeInOut
        });
    
        TweenMax.to(newSlide,1,{
          width:slideshow.width(),
          ease:Power3.easeInOut
        });
    
        TweenMax.to(newSlideImage,1,{
          right:newSlideImageToRight,
          left:newSlideImageToLeft,
          ease:Power3.easeInOut
        });
    
        TweenMax.staggerFromTo(newSlideElements,0.8,{alpha:0,y:60},{alpha:1,y:0,ease:Power3.easeOut,force3D:true,delay:0.6},0.1,function(){
          newSlide.addClass('is-active1').removeClass('is-new1');
          activeSlide.removeClass('is-active1');
          newSlide.css({
            display:'',
            width:'',
            left:'',
            zIndex:''
          });
    
          newSlideImage.css({
            width:'',
            right:'',
            left:''
          });
    
          newSlideContent.css({
            width:'',
            left:''
          });
    
          newSlideElements.css({
            opacity:'',
            transform:''
          });
    
          activeSlideImage.css({
            left:''
          });
    
          slideshow.find('.pagination1').trigger('check');
          slideshow.data('wait',false);
          if(auto){
            timeout=setTimeout(function(){
              slideshowNext(slideshow,false,true);
            },slideshowDuration);
            slideshow.data('timeout',timeout);
          }
        });
      }
    }
    
    function slideshowNext(slideshow,previous,auto){
      var slides=slideshow.find('.slide1');
      var activeSlide=slides.filter('.is-active1');
      var newSlide=null;
      if(previous){
        newSlide=activeSlide.prev('.slide1');
        if(newSlide.length === 0) {
          newSlide=slides.last();
        }
      } else {
        newSlide=activeSlide.next('.slide1');
        if(newSlide.length==0)
          newSlide=slides.filter('.slide1').first();
      }
    
      slideshowSwitch(slideshow,newSlide.index(),auto);
    }
    
    function homeSlideshowParallax(){
      var scrollTop=$(window).scrollTop();
      if(scrollTop>windowHeight) return;
      var inner=slideshow.find('.slideshow-inner1');
      var newHeight=windowHeight-(scrollTop/2);
      var newTop=scrollTop*0.8;
    
      inner.css({
        transform:'translateY('+newTop+'px)',height:newHeight
      });
    }
    
    $(document).ready(function() {
     $('.slide1').addClass('is-loaded1');
    
     $('.slideshow1 .arrows1 .arrow1').on('click',function(){
      slideshowNext($(this).closest('.slideshow1'),$(this).hasClass('prev1'));
    });
    
     $('.slideshow1 .pagination1 .item1').on('click',function(){
      slideshowSwitch($(this).closest('.slideshow1'),$(this).index());
    });
    
     $('.slideshow1 .pagination1').on('check',function(){
      var slideshow=$(this).closest('.slideshow1');
      var pages=$(this).find('.item1');
      var index=slideshow.find('.slides1 .is-active1').index();
      pages.removeClass('is-active1');
      pages.eq(index).addClass('is-active1');
    });
    
    /* Lazyloading
    $('.slideshow').each(function(){
      var slideshow=$(this);
      var images=slideshow.find('.image').not('.is-loaded');
      images.on('loaded',function(){
        var image=$(this);
        var slide=image.closest('.slide');
        slide.addClass('is-loaded');
      });
    */
    
    var timeout=setTimeout(function(){
      slideshowNext(slideshow,false,true);
    },slideshowDuration);
    
    slideshow.data('timeout',timeout);
    });
    
    if($('.main-content1 .slideshow1').length > 1) {
      $(window).on('scroll',homeSlideshowParallax);
    }
    </script>
